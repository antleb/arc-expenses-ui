{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "User",
  "type": "object",
  "properties": {
    "id": { "type": "string" },
    "email": { "type": "string" },
    "firstname": { "type": "string" },
    "lastname": { "type": "string" },
    "firstnameLatin": { "type": "string" },
    "lastnameLatin": { "type": "string" },
    "signatureArchiveId": { "type": "string" },
    "receiveEmails": { "type": "string" },
    "immediateEmails": { "type": "string" },
    "attachment": {
      "type": "object",
      "properties": {
        "filename": { "type": "string" },
        "mimetype": { "type": "string" },
        "size": { "type": "integer" },
        "url": { "type": "string" }
      }
    }
  },
  "required": [
    "id",
    "email",
    "firstname",
    "lastname"
  ]
}